<header class="back-plate yield-container">
    <%= link_to "/", class: "navbar-brand" do %>
      <%= image_tag "https://res.cloudinary.com/ddt8brfwz/image/upload/v1712761010/Muscle_j3izpc.svg", height: "50" %>
    <% end %>

    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

  <ul>
    <li class="s-bold-title"><%= link_to 'Cars', cars_path %></li>
    <li class="s-bold-title"><%= link_to 'Rent my Car', new_car_path %></li>
    <li class="s-bold-title"><%= link_to 'My Bookings', bookings_path %></li>
    <li class="s-bold-title"><%= link_to 'My Cars', my_cars_path %>
  </ul>

  <% if current_user %>
    <div class="login-register-container">
      <%= link_to 'Logout', destroy_user_session_path, class: "secondary-cta", data: { turbo_method: :delete } %>
    </div>
  <% else %>
    <div class="login-register-container">
        <%= link_to 'Login', new_user_session_path, class: "secondary-cta" %>
        <%= link_to 'Register', new_user_registration_path, class: "main-cta" %>
    </div>
  <% end %>
  </header>
