<!-- app/views/cars/my_cars.html.erb -->
<h1>My Cars</h1>

<% @my_cars.each do |car| %>
  <div>
    <h2><%= car.brand %> <%= car.model %></h2>
    <p>Year: <%= car.year_of_production %></p>
    <p>Price per day: <%= car.price_per_day %> €</p>
    <!-- Ajoutez plus de détails selon votre modèle -->
  </div>
<% end %>
